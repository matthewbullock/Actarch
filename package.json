{
  "name": "actearch",
  "version": "1.0.0",
  "engines": {
      "node": "7.4.0"
   },
  "scripts": {
    "start": "node backend/app.js",
    "build": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "heroku-postbuild": "yarn build",
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "mongoose": "^5.3.4"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
